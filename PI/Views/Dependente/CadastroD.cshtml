<style>
    #site {
        width: 100%;
        height: 100%;
        margin-top: 0;
        margin-left: auto;
        margin-bottom: 0;
        margin-right: auto;
        background-color: aliceblue;
    }

    #header {
        width: 100%;
        height: 50px;
        text-align: center;
    }

    #conteudo {
        width: 100%;
        height: 500px;
    }

    #conteudo-left {
        width: 50%;
        height: 100%;
        float: left;
    }

    #conteudo-right {
        width: 50%;
        height: 80%;
        float: left;
    }

    .col-25 {
        float: left;
        width: 20%;
        margin-top: 5px;
        display: inline-block;
        text-align: center;
    }

    .col-75 {
        float: left;
        width: 75%;
        margin-top: 5px;
    }

    input[type=text],
    input[type=date],
    input[type=email] {
        width: 100%;
        padding: 12px;
        border: 3px solid #808080;
        border-radius: 4px;
        resize: vertical;
    }

    .alinhar {
        text-align: center;
        margin-top: 20%;
    }

    button {
        background-color: green;
        padding: 12px 20px;
        margin-top: 10px;
        color: white;
        border: none;
        border-radius: 7px;
        cursor: pointer;
        float: left;
    }

        button:hover {
            background-color: rgb(6, 255, 6);
        }
</style>


<div id="site">

    <div id="header">
        <h3>CADASTRE UM DEPENDENTE</h3>
    </div>
    <div id="conteudo">
        <form>
            <div id="conteudo-left">
                <div class="col-25">
                    <label for=" nome">Nome</label>
                </div>
                <div class="col-75">
                    <input type="text" name="nome" maxlength="200" />
                </div>

                <div class="col-25">
                    <label for=" cpf">CPF</label>
                </div>
                <div class="col-75">
                    <input type="text" name="cpf" minlength="11" maxlength="14" />
                </div>

                <div class="col-25">
                    <label for=" email">Email</label>
                </div>
                <div class="col-75">
                    <input type="email" name="email" />
                </div>

                <div class="col-25">
                    <label for=" senhaD">Senha Dependente</label>
                </div>
                <div class="col-75">
                    <input type="text" name="senhaD" />
                </div>

                <div class="col-25">
                    <label for=" senhaDR">Repetir Senha</label>
                </div>
                <div class="col-75">
                    <input type="text" name="senhaDR" />
                </div>

                <div class="col-25">
                    <label for=" dataN">Data Nascimento</label>
                </div>
                <div class="col-75">
                    <input type="date" name="dataN" />
                </div>

                <div class="col-25">
                    <label for=" cep">CEP</label>
                </div>
                <div class="col-75">
                    <input type="text" name="cep" minlength="8" maxlength="9" />
                </div>

                <div>
                    <button type="button" id="submitBtn">Cadastrar</button>
                </div>

            </div>
        </form>

        <form>
            <div id="conteudo-right">
                <div class="alinhar">
                    <h5><u>Validação Titular</u></h5>
                </div>

                <div class="col-25">
                    <label for=" cpf">CPF</label>
                </div>
                <div class="col-75">
                    <input type="text" name="cpf" minlength="11" maxlength="14" />
                </div>

                <div class="col-25">
                    <label for=" senhaT">Senha</label>
                </div>
                <div class="col-75">
                    <input type="text" name="senhaT" />
                </div>
            </div>
        </form>
    </div>
</div>